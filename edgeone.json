{
  "name": "api-init",
  "build": {
    "command": "npm install",
    "output": "dist"
  },
  "routes": [
    {
      "pattern": ".*",
      "script": "functions/[[path]].js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api": {
      "memory": 128,
      "maxDuration": 10
    }
  }
}